{
    "name": "cesarbosqueiro/laravel-default-config",
    "description": "My personal coding standards for a Laravel project.",
    "keywords": [
        "laravel",
        "laravel-default-config"
    ],
    "homepage": "https://github.com/cesarbosqueiro/laravel-default-config",
    "license": "MIT",
    "authors": [
        {
            "name": "Bosqro",
            "email": "bosqro@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^8.1.1||^7.10.0"
    },
    "autoload": {
        "psr-4": {
            "Bosqro\\LaravelDefaultConfig\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bosqro\\LaravelDefaultConfig\\LaravelCodeStandardsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
